@model List<ProjectViewModel>



<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-md-4">
            <div class="project img ftco-animate d-flex justify-content-center align-items-center" style="background-image: url(@item.ImageUrl);">
                <div class="overlay"></div>
                <div class="text text-center p-4">
                    <h3><a href="@item.GithubUrl">@item.Title</a></h3>
                    <span>@item.Tags</span>
                </div>
            </div>
        </div>
    }
</div>